{
  "mcpServers": {
    "playwriter": {
      "command": "npx",
      "args": ["-y", "playwriter@latest"],
      "lifecycle": "lazy",
      "env": {
        "PLAYWRITER_AUTO_ENABLE": "1"
      }
    },
    "sentry": {
      "url": "https://mcp.sentry.dev/mcp",
      "auth": "oauth",
      "lifecycle": "lazy"
    }
  }
}
