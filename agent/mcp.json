{
  "mcpServers": {
    "playwriter": {
      "command": "npx",
      "args": ["-y", "playwriter@latest"],
      "lifecycle": "lazy",
      "env": {
        "PLAYWRITER_AUTO_ENABLE": "1"
      }
    },
    "sentry": {
      "command": "npx",
      "args": ["-y", "mcp-remote", "https://mcp.sentry.dev/mcp"],
      "lifecycle": "lazy"
    },
    "vercel": {
      "command": "npx",
      "args": ["-y", "mcp-remote", "https://mcp.vercel.com"],
      "lifecycle": "lazy"
    }
  }
}
